<resources>
<style name="FacebookLoginButton">
    <!-- <item name="android:textSize">@dimen/smallTxtSize</item> -->
    <item name="android:background">@drawable/register_facebook</item>
    <!-- <item name="android:layout_marginTop">10dp</item>
    <item name="android:layout_marginBottom">10dp</item>
    <item name="android:layout_gravity">center_horizontal</item> -->
</style>
    <style name="MyTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/MyTheme.ActionBarStyle</item>
        <!-- <item name="android:actionButtonStyle">@style/ActionButtonStyle</item> -->
    </style>

    <style name="MyTheme.ActionBarStyle" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:titleTextStyle">@style/MyTheme.ActionBar.TitleTextStyle</item>
        <!-- <item name="android:actionButtonStyle">@style/ActionButtonStyle</item> -->
        <item name="android:background">@color/text_edit</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:icon">@android:color/transparent</item>
    </style>

    <style name="ActionButtonStyle" parent="@android:style/Theme.Holo.Light">
        <item name="android:minWidth">50dip</item>
        <item name="android:paddingLeft">0dip</item>
        <item name="android:paddingRight">0dip</item>
    </style>

    <style name="MyTheme.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="MyListBackContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="MyListFrontContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:padding">8dp</item>
    </style>

    <color name="custom_theme_color">#00000000</color>

    <style name="CustomTheme" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@color/custom_theme_color</item>
    </style>

</resources>